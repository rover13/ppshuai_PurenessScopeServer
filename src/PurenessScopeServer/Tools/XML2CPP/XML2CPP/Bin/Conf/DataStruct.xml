		<Temperature desc="温度">uint8</Temperature>
		<StatusCode desc="状态码">uint8</StatusCode>
		<ErrorCode desc="错误码">uint8</ErrorCode>
	</Smart_Share_Bicycle_Param_Body>

	<Smart_Share_Bicycle_Param desc="共享单车参数上报">
